    If bDebug = -1 Then
        On Error GoTo 0
    ElseIf bDebug = 1 Then
        On Error Resume Next
    Else
        On Error GoTo ErrorOccur
    End If


    Select Case Index
    Case 0
        Select Case KeyAscii
        Case 48 To 57, 8
        Case Asc(".")
            If InStr(txtAmount(0).Text, ".") > 0 Then KeyAscii = 0
        Case Else
            KeyAscii = 0
        End Select
    End Select
    
Exit Sub



