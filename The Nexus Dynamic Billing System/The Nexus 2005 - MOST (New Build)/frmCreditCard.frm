Private Sub cmdName_Click()


    '*[ Error Checking Variables ]**********************************************************************************
    
    
    Const RoutineName = "cmdName_Click"
    Const ContainerName = "frmCreditCard"
    '***************************************************************************************************************


'
'***********************************************************************************************
'**  Project Alpha ® 2003, 2004 +                                                             **
'***********************************************************************************************
'**  This code is not to be distributed, reverse engineered or simulated in any way without   **
'**  Premission from the author. The authors of this code is as follows: Simon Antony Roberts **                                                     **
'**Jarrett Cliff Costi, these two are the only people you can communicate with about this code**
'***********************************************************************************************
'**  Project Alpha is a product of Exitstencil Press Australia                                **
'***********************************************************************************************
'**                                                                                           **
'**  Routine:                                                                                 **
'**  Arguments:                                                                               **
'**  Description:    Subroutine, Function or Property of project alpha                        **
'**  Author:         Simon Roberts                                                            **
'**  Date Last mod:  19-01-2004                                                               **
'**                                                                                           **
'********************************************** Copyright © 2004 Exitstencil Press Australia ***
'
'
'
    If bDebug = -1 Then
        On Error GoTo 0
    ElseIf bDebug = 1 Then
        On Error Resume Next
    Else
        On Error GoTo ErrorOccur
    End If


    Dim bResult As Boolean
    Dim rsLoad As ADODB.Recordset
    
    Select Case cmdName.ListIndex
    Case -1
    Case Else
        bResult = MySQL.OpenTable(ADOConn, rsLoad, , "select AES_DECRYPT(CardNumber,'" + odb.colSalts.ReturnSalt("CCSalt") + "" & odb.colSalts.ReturnSalt("md5Password") & "') as CardNumber, RecID, SecurityNumber, ExpiryDate, bType, bDefault " + _
                                                      "from creditcard Where RecID = " & cmdName.ItemData(cmdName.ListIndex))
        If rsLoad.RecordCount > 0 Then
            txtField(1).Text = IIf(IsNull(rsLoad!CardNumber), "", MySQL.NumDecrypt(rsLoad!CardNumber))
            txtField(2).Text = IIf(IsNull(rsLoad!SecurityNumber), "", rsLoad!SecurityNumber)
            dtpExpiry.Value = IIf(IsNull(rsLoad!ExpiryDate), #9/19/1950#, rsLoad!ExpiryDate)
            txtField(3).Text = ""
            cmbType.ListIndex = rsLoad!bType
            If IsNull(rsLoad!bDefault) Then
                chkDefault.Value = 1
            Else
                chkDefault.Value = IIf(Val(rsLoad!bDefault) < 0, 1, 0)
            End If
        End If
    End Select
    
Exit Sub



ErrorOccur:
Select Case oErr.chkError(Val(Err.Number), Err.Description, RoutineName, ContainerName)
Case vbResume
    Resume
Case vbExit
    
Case vbResumeNext
    Resume Next
End Select

End Sub

